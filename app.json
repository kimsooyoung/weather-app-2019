    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "description": "Simple weather App with React Native",
    "githubUrl": "https://github.com/kimsooyoung/weather-app-2019"
  }
}
