{
  "expo": {
    "name": "Weather App 2019",
    "slug": "weather-app-2019",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
